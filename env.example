# Configurações do Supabase
# URL do seu projeto Supabase.
# NEXT_PUBLIC_ é para variáveis acessíveis no frontend.
NEXT_PUBLIC_SUPABASE_URL="https://seu-projeto.supabase.co"
SUPABASE_URL="https://seu-projeto.supabase.co"

# Chave anônima pública do seu projeto Supabase.
# NEXT_PUBLIC_ é para variáveis acessíveis no frontend.
NEXT_PUBLIC_SUPABASE_ANON_KEY="sua-chave-anonima-aqui"
SUPABASE_ANON_KEY="sua-chave-anonima-aqui"

# Chave de serviço do Supabase. Use com cautela, pois possui privilégios elevados.
# NÃO deve ser exposta no frontend.
SUPABASE_SERVICE_ROLE_KEY="sua-chave-de-servico-aqui"

# Configurações da Meta WhatsApp Cloud API
# ID do número de telefone configurado na Meta.
META_PHONE_NUMBER_ID="your_phone_number_id_here"
# Token de acesso da API do WhatsApp Business.
META_ACCESS_TOKEN="your_access_token_here"
# Token de verificação para webhooks do WhatsApp.
META_VERIFY_TOKEN="your_verify_token_here"
# Versão da API do WhatsApp a ser utilizada.
WHATSAPP_API_VERSION="v20.0"

# Configurações da OpenAI (para o Agente de IA)
# Chave da API da OpenAI.
OPENAI_API_KEY="your_openai_api_key_here"
# Modelo da OpenAI a ser utilizado (ex: gpt-4, gpt-4o-mini).
OPENAI_MODEL="gpt-4o-mini"
# Temperatura do modelo (0.0 a 1.0). Controla a aleatoriedade das respostas.
OPENAI_TEMPERATURE="0.3"

# Configurações do Google Calendar (opcional, se for usar Service Account)
# JSON da Service Account do Google codificado em Base64 (sem quebras de linha).
# Gerado no Google Cloud Console para autenticação programática.
GOOGLE_SA_JSON_BASE64=""
# ID do calendário a ser utilizado (ex: 'primary' para o calendário principal).
GOOGLE_CALENDAR_ID="primary"

# Configurações do PostgreSQL (para o gerenciador de jobs pg-boss)
# URL de conexão completa com o banco de dados PostgreSQL.
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DBNAME"

# Configurações Gerais da Aplicação
# URL pública da aplicação (usada para callbacks, etc.).
NEXT_PUBLIC_APP_URL="http://localhost:3000"
# Fuso horário da aplicação.
TZ="America/Sao_Paulo"

